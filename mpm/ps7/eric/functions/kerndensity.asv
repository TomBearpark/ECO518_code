function out = kerndensity(X, XI, h)
% Input
%   - X data (estimation)
%   - XI

%%
X  = X(:);
XI = XI(:);

sigma = std(X);
N     = length(X);
NGrid = length(XI);


if ~exist('h')
    h = 1.059 * sigma / N^(1/5);
end



%%

XIMat = repmat(XI, 1, N);
XMat  = repmat(X', NGrid, 1); 

XMat

normpdf()



end